#!/bin/bash


[[ -e /bin/xdarnix/msg ]] && source /bin/xdarnix/msg || source <(curl -sSL https://raw.githubusercontent.com/darnix1/license/main/msg)


msg -bar
msg -tit
msg -bar
amacen " CONFIGURACION VMESS CLIENTES "
echo ""
menu_func "-vd Este es un mensaje verde" 
"-vm Este es un mensaje rojo" 
"-fi Primero Segundo" 
"-bar Una barra"
back
echo -e ""
